{
  "name": "LaraStack",
  "version": "1.0.0",
  "description": "Official Discord bot of 'Stack de programadores' community",
  "main": "index.js",
  "repository": "https://github.com/Upping-Devs/LaraStack.JS",
  "author": "Weslley Borges <60989781+Weslley-Borges@users.noreply.github.com>",
  "license": "MIT",

  "scripts":{
    "commit": "git-cz"
  },

  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^4.3.0",
    "init": "0.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
